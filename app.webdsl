website {
    name "WebDSL"
    author "William Cotton"
    version "0.0.1"
    port $PORT
    database $DATABASE_URL
    auth {
        salt $SALT
        github {
            clientId $GITHUB_CLIENT_ID
            clientSecret $GITHUB_CLIENT_SECRET
        }
    }
    include "auth.webdsl"
    include "api.webdsl"
    include "pages.webdsl"
    include "htmx.webdsl"
    include "notes.webdsl"
}
